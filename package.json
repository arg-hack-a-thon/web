{
  "name": "web",
  "version": "1.0.0",
  "description": "The admin",
  "scripts": {
    "clean": "rimraf static",
    "build:webpack": "NODE_ENV=production webpack --config ./webpack/config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "node ./node_modules/better-npm-run start",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "lint": "eslint src",
    "test": "node ./node_modules/better-npm-run test"
  },
  "betterScripts": {
    "start": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8008
      }
    },
    "start-dev": {
      "command": "node ./server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 8008
      }
    },
    "test": {
      "command": "mocha --compilers js:babel-core/register --require ./test/testHelper.js src/**/*.spec.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "test",
        "PORT": 8008
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arg-hack-a-thon/web"
  },
  "author": "ZG Labs",
  "bugs": {
    "url": "https://github.com/arg-hack-a-thon/web/issues"
  },
  "homepage": "https://github.com/arg-hack-a-thon/web",
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "babel-plugin-react-transform": "^1.1.1",
    "better-npm-run": "0.0.4",
    "bootstrap": "^3.3.5",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.19.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^2.3.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "image-webpack-loader": "^1.6.1",
    "imagemin": "^3.2.0",
    "imports-loader": "^0.6.4",
    "jsdom": "^7.0.2",
    "json-loader": "^0.5.3",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "mocha": "^2.3.4",
    "postcss-loader": "^0.6.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.3",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.0",
    "redbox-react": "^1.0.1",
    "redux-devtools": "3.0.0-beta-3",
    "redux-devtools-dock-monitor": "^1.0.0-beta-3",
    "redux-devtools-log-monitor": "^1.0.0-beta-3",
    "rimraf": "^2.4.3",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "component-cookie": "^1.1.2",
    "confidence": "^1.4.2",
    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "fixed-data-table": "^0.4.7",
    "font-awesome": "^4.4.0",
    "hash-files": "^1.0.0",
    "history": "^1.13.0",
    "immutable": "^3.7.5",
    "jquery": "^2.1.4",
    "query-string": "^3.0.0",
    "react": "^0.14.0",
